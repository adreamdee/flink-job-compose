{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "parallel": {
      "type": "integer",
      "description": "独立配置算子的并发度",
      "default": 0
    },
    "filepath": {
      "type": "string"
    },
    "linesinterval": {
      "type": "integer",
      "default": 0,
      "description": "lines interval sleep millseconds"
    }
  },
  "required": ["filepath"]
}